{
  "name": "slapjs-com",
  "version": "1.0.0",
  "main": "server.js",
  "author": "Christopher Hacia",
  "description": "A robust and agile boilerplate for rapid web development using Node.js, Express.js, React.js, MySQL, Bootstrap, SASS, and more.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/slapjs.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "react",
    "mysql",
    "bootstrap",
    "sass"
  ],
  "scripts": {
    "start": "ts-node server.ts",
    "dev:backend": "nodemon server.ts",
    "dev:client": "webpack --watch",
    "styles:watch": "sass --watch shared/scss:shared/styles",
    "lint:js": "eslint .",
    "lint:css": "stylelint shared/scss/**/*.scss",
    "prepare:husky": "husky install",
    "lint-staged": "lint-staged",
    "start:all": "concurrently \"npm run dev:backend\" \"npm run dev:client\" \"npm run styles:watch\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.5.1",
    "@types/react-bootstrap": "^0.32.36",
    "axios": "1.6.2",
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.2",
    "bootstrap": "5.3.2",
    "bootstrap-icons": "1.11.3",
    "compression": "1.7.4",
    "concurrently": "8.2.2",
    "cors": "2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "3.1.9",
    "email-validator": "2.0.4",
    "express": "4.18.2",
    "express-handlebars": "7.1.2",
    "express-jwt": "8.4.1",
    "express-session": "1.17.3",
    "helmet": "7.1.0",
    "html-webpack-plugin": "5.6.0",
    "jquery": "3.7.1",
    "jsonwebtoken": "9.0.2",
    "jwt-decode": "^4.0.0",
    "lodash": "4.17.21",
    "mysql2": "^3.9.8",
    "node-sass": "9.0.0",
    "node-sass-middleware": "1.1.0",
    "nodemailer": "6.9.7",
    "passport": "0.7.0",
    "passport-local": "1.0.0",
    "react": "18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "18.2.0",
    "sass": "1.69.5",
    "sendgrid": "5.2.3",
    "sequelize": "6.35.1",
    "swagger-ui-express": "5.0.0",
    "winston": "3.11.0",
    "xss-filters": "^1.2.7",
    "yamljs": "0.3.0",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "^7.24.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.5",
    "@types/xss-filters": "^0.0.30",
    "@types/yamljs": "^0.2.34",
    "@types/zxcvbn": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-loader": "9.1.3",
    "eslint": "7.32.0 || 8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-security": "1.7.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.2.0",
    "nodemon": "3.0.1",
    "stylelint": "16.2.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "supertest": "6.3.3",
    "ts-node": "10.9.2",
    "typescript": "^5.1.6",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ],
    "*.tsx": [
      "eslint --fix"
    ],
    "*.css": [
      "stylelint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=18.10.0",
    "npm": ">=8.19.2"
  }
}
